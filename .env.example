# Content Builder Agent Environment Variables

# Required: LLM Configuration (DashScope/Qwen)
DASHSCOPE_API_KEY=your_dashscope_api_key
DEFAULT_LLM_MODEL=qwen-max

# Required: Web Search (Tavily)
TAVILY_API_KEY=your_tavily_api_key

# Optional: LangSmith Observability
LANGSMITH_API_KEY=your_langsmith_key
LANGSMITH_TRACING=true

# Optional: Content Extraction (Firecrawl)
FIRECRAWL_API_KEY=your_firecrawl_api_key

# Optional: Social Media Publishing
# Twitter/X
TWITTER_APP_KEY=
TWITTER_APP_SECRET=
TWITTER_ACCESS_TOKEN=
TWITTER_ACCESS_SECRET=

# LinkedIn
LINKEDIN_ACCESS_TOKEN=
LINKEDIN_PERSON_URN=
